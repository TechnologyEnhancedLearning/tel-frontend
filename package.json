{
  "name": "tel-frontend-monorepo",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build:package": "cd packages/tel-frontend && npm run build",
    "build:review": "cd packages/tel-frontend-review && node build.mjs",
    "build": "npm run build:package && npm run build:review",
    "lint": "eslint . --ext .js,.ts,.vue",
    "prettier": "prettier --check ."
  },
  "dependencies": {
    "nunjucks": "^3.2.4",
    "fs-extra": "^11.1.1",
    "sass": "^1.72.1"
  },
  "devDependencies": {
    "eslint": "^8.0.0",
    "prettier": "^3.0.0"
  },
  "engines": {
    "node": ">=18"
  }
}
